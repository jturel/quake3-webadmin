{
  "name": "quake3-webadmin",
  "version": "0.1.0-snapshot",
  "private": true,
  "main": "electron.js",
  "description": "Quake 3 Server Web UI",
  "homepage": ".",
  "author": "Jonathon Turel <jturel@gmail.com>",
  "build": {
    "appId": "quake3-webadmin",
    "files": [
      "build/**/*"
    ],
    "linux": {
      "publish": [
        "github"
      ],
      "target": [
        "AppImage"
      ]
    }
  },
  "dependencies": {
    "@rebass/forms": "^4.0.6",
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "nocache": "^3.0.4",
    "pouchdb": "^7.3.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "rebass": "^4.0.7",
    "winston": "^3.7.2"
  },
  "scripts": {
    "start": "BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "package": "npm run build && cp electron.js build/ && mkdir build/src && cp src/server -R build/src/ && cp src/shared -R build/src/ && electron-builder build",
    "test": "NODE_ENV=test react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint ./src/",
    "electron": "electron ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "electron": "^19.0.11",
    "electron-builder": "^23.0.3",
    "nodemon": "^2.0.16",
    "supertest": "^6.2.3"
  }
}
